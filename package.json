{
  "name": "react-quickstart",
  "version": "1.4.0",
  "scripts": {
    "fmt:check": "npx prettier --single-quote -c src",
    "fmt:fix": "npx prettier --single-quote -w src",
    "test": "react-scripts test",
    "test:all": "react-scripts test --watchAll=false",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "preversion": "git fetch --tags && yarn test-all",
    "version": "yarn build",
    "postversion": "git push --follow-tags"
  },
  "dependencies": {
    "animate.css": "^4.1.1",
    "query-string": "^7.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.2",
    "react-scripts": "^4.0.3",
    "web-vitals": "^1.1.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "author": "Alejandro Gallego",
  "license": "MIT",
  "homepage": "https://camaleongrafico.com/",
  "description": "> Starter template for a React app - including docs, CI, and hosting",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/camgrafiman/superheroes.git"
  },
  "bugs": {
    "url": "https://github.com/camgrafiman/superheroes/issues"
  }
}
